APP_READ_ONLY=false
APP_NAME="Laravel Boilerplate"
APP_ENV=local
APP_KEY=
APP_TESTING=false
APP_DEBUG=true
APP_URL=http://localhost
APP_LOCALE=es
APP_FAKER_LOCALE=es_ES
APP_FALLBACK_LOCALE=es
