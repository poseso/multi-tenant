TENANCY_DB_USERNAME=homestead
TENANCY_DB_PASSWORD=secret
TENANCY_CACHE_TTL=10
TENANCY_DATABASE_AUTO_DELETE=false
TENANCY_DATABASE_AUTO_DELETE_USER=false
TENANCY_EARLY_IDENTIFICATION=true
TENANCY_AUTO_HOSTNAME_IDENTIFICATION=true
TENANCY_ABORT_WITHOUT_HOSTNAME=true
AUTO_DELETE_TENANT_DIRECTORY=true

LIMIT_UUID_LENGTH_32=true

MYSQL_DUMP_PATH=/Applications/MAMP/Library/bin/
DUMP_USING_SINGLE_TRANSACTION=true

BROADCAST_DRIVER=log
CACHE_DRIVER=file
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
